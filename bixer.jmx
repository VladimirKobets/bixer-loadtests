            <boolProp name="resetInterpreter">false</boolProp>
            <stringProp name="script">import javax.crypto.Mac;
import javax.crypto.spec.SecretKeySpec;
import java.io.IOException;
import java.text.ParseException;
import java.math.BigInteger;
import java.util.*;
import java.security.InvalidKeyException;
import org.apache.commons.codec.binary.Hex;

String side = &quot;sell&quot;;
String accessKey = &quot;PTBes9Gjez5AnGbfNZvhaFBrns9lydx68ytSJiqU&quot;;
String secretKey = &quot;alfTdhkwQpdns2uz0WKkxcnGHYaCKojl3XNWOJhf&quot;;

long timestamp = System.currentTimeMillis();
String params = &quot;access_key=&quot; + accessKey + &quot;&amp;market=ethbtc&amp;price=1&amp;side=&quot; + side + &quot;&amp;tonce=&quot; + timestamp + &quot;&amp;volume=1&quot;;
String data = &quot;POST|/api/v2/orders|&quot; + params;

// Sign message
Mac mac = Mac.getInstance(&quot;HmacSHA256&quot;);
SecretKeySpec secretKeySpec = new SecretKeySpec(secretKey.getBytes(&quot;UTF-8&quot;), &quot;HmacSHA256&quot;);
mac.init(secretKeySpec);
mac.update(data.getBytes(&quot;UTF-8&quot;));
String sign = new BigInteger(1, mac.doFinal()).toString(16);

vars.put(&quot;NEW_DATA&quot;, params + &quot;&amp;signature=&quot; + sign);</stringProp>
          </BeanShellPreProcessor>
